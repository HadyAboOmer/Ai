def Booster():
    global gameImage
    global gameName
    global gamePath
    hour = StringVar()
    minute = StringVar()
    second = StringVar()
    hour.set("00")
    minute.set("03")
    second.set("00")
    def submit():
        os.startfile(r"D:\Software\Software\gexagonvr-ultimate-booster\UltimateBooster\UltimateBooster.exe")                                                  #******  Game Path  *******
        temp = int(hour.get()) * 3600 + int(minute.get()) * 60 + int(second.get())
        while temp > -1:
            mins, secs = divmod(temp, 60)
            hours = 0
            if mins > 60:
                hours, mins = divmod(mins, 60)
            hour.set("{0:2d}".format(hours))
            minute.set("{0:2d}".format(mins))
            second.set("{0:2d}".format(secs))
            root.update()
            time.sleep(1)
            if (temp == 120):
                connect = sqlite3.connect("2000db.db")
                c = connect.cursor()
                c.execute("CREATE TABLE if not exists Booster (Game_ClickCount integer)")
                c.execute("INSERT INTO  Booster (Game_ClickCount) values (1) ")
                c.execute('select * from Booster')
                result = c.fetchall()
                print(len(result))
                connect.commit()
                connect.close()
            if (temp == 0):
                messagebox.showinfo("Time Countdown", "Time's up ")
            temp -= 1
        else:
            os.system("taskkill /im UltimateBooster.exe")                             #******  Game Path  *******
            # time.sleep(1)
            return Tap1()
    gameImage = PhotoImage(file="BOOSTER.png")
    gameName = str("Booster")

    BG_F = customtkinter.CTkFrame(master=root, width=1920, height=1300, border_width=0, corner_radius=15,
