                messagebox.showinfo("Time Countdown", f"Time's up ")
            temp -= 1
        else:
            os.system("taskkill /im UE4Game-Win64-Test.exe")
            # time.sleep(1)
            return Tap1()
    gameImage = PhotoImage(file="BULLET-TRAIN.png")
    gameName = str("Bullet Train")

    BG_F = customtkinter.CTkFrame(master=root, width=1920, height=1300, border_width=0, corner_radius=15,
