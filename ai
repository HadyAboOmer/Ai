def destroy():
    root.destroy()
#*******************************************  Control Panel  **************************************************************

def setting():
    bg_masrt_F = Frame(root, width=1920, height=1300, bg="#242628").place(x=0, y=0)
    Logo_L = Label(root, width=1920, height=70, bg="#0453a2", image=meta).place(x=0, y=0)
    home_B = Button(root, borderwidth=0, bg='#0453a2', width=3,height=1, text=" ðŸ”³ ", font=('', 25), fg='white',
             activebackground='#0453a2', activeforeground="white", command=Tap1).place(x=60, y=2)
    clos_B = customtkinter.CTkButton(master=root, width=1, height=1, fg_color="#0453a2", bg_color="#0453a2",hover_color="red",
             text="âœ–", text_font=('', 14, ""), text_color="white", command=destroy).place(x=1855,y=23)
    welcom_L = Label (root,text=f"Welcome Back : {username_verify.get()}", bg="#0453a2", font=('', 15),
             fg='white').place(x=70,y=310)

    Beat_Saber_query()
    Bullet_Train_query()
    Booster_query()
    Jop_Simulator_query()
    Roborecall_query()
    Richies_Plank_query()
    Face_Your_Fears_query()
    Coaster_query()
    Spider_Man_query()
    Over_Take_Vr_query()
    Into_The_Dead_query()
    War_Robots_Vr_query()
    Fruit_Ninja_query()
    The_Mental_Torment_query()
    Roller_Coasters_query()
    Gray_query()
    Arizona_Sunshine_query()
    Minecraft_query()
    Henry_query()
    Demonic_Guest_Vr_query()

#*******************************************  Games Query  **************************************************************

def Beat_Saber_query():
    global a
    global c
    global c1
    connect = sqlite3.connect("2000db.db")
    cursor = connect.cursor()
    cursor.execute('select * from Beat_Saber')
    result = cursor.fetchall()
    a = len(result)
    c1=a
    b = a * 20
    c = b
    print(c1,c)
    connect.commit()
    connect.close()
    # ------------------------------------------------------------------------------


    # ------------------------------------------------------------------------------
             #                                        #                          #
    sumclick_L = Label(root, text=f"   Game Name                             Clicks                  Money ",
                       bg='#0453a2', font=('', 15), fg='white').grid(row=1, column=1, padx=50, pady=30)
    sumclick_L = Label(root, text=f" Beat Saber                                   {a}                        {b} $  "
                       , bg='#242628', font=('', 15), fg='white').grid(row=2, column=1)  #
    leftuprow = Label(root, width=2, height=2, bg="#0453a2").grid(row=0, column=0, padx=350, pady=37)

