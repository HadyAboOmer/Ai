def Demonic_Guest_Vr_query():
    global a
    global v
    global c20
    global nm
    global all_money
    global largnum
    global allmoney
    global Top_play
    connect = sqlite3.connect("2000db.db")
    cursor = connect.cursor()
    cursor.execute('select * from Demonic_Guest_Vr')
    result = cursor.fetchall()
    a = len(result)
    # name = result[0]
    # Name = name[1]
    # print(Name)
    c20 = a
    b = a * 15
    v = b

    all_money =(c + d + e + f + g + h + i + j + k + l + m + n + o + p + q + r + s + t + u + v)      # all tables variable in one variable #allmoney in SQL c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v
    all_clicks =(c1+c2+c3+c4+c5+c6+c7+c8+c9+c10+c11+c12+c13+c14+c15+c16+c17+c18+c19+c20)

    connect.commit()
    connect.close()

    Top_play = (c1,c2,c3,c4,c5,c6,c7,c8,c9,c10,c11,c12,c13,c14,c15,c16,c17,c18,c19,c20)  # all tables variable in one variable #allmoney in SQL
    largnum = max(Top_play)
    print(f"Top Game Clicks {largnum} is name is  :  ")

    # for number in largnum:  # For get heights Number from all tables in SQL
    #     if number > largnum:
    #         largnum = number
    #     print(largnum)
    # connect.commit()

    #                                      #                                        #                          #

    sumclick_L = Label(root, text=f"   Game Name                             Clicks                  Money ",
                       bg='#0453a2', font=('', 15), fg='white').grid(row=1, column=1, padx=50, pady=30)
    sumclick_L = Label(root, text=f" Demonic_Guest_Vr                                 {a}                        {b} $  "
                       , bg='#242628', font=('', 15), fg='white').grid(row=21 , column=1)
