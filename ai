#*******************************************  Login Setting   **************************************************************

def login():
    BG_F = customtkinter.CTkFrame(master=root, width=1920, height=1300, border_width=0, corner_radius=15,
                                  fg_color="#131313", bg_color="#161616").place(x=0, y=75)
    BG_F1 = customtkinter.CTkFrame(master=root, width=575, height=400, border_width=0, corner_radius=15,
                                   fg_color="#1A1A1A", bg_color="#131313").place(x=689, y=210)
    BG_F2 = customtkinter.CTkFrame(master=root, width=550, height=375, border_width=0, corner_radius=15,
                                   fg_color="#1e1e1e", bg_color="#1A1A1A").place(x=700, y=225)
    BG_F2 = customtkinter.CTkFrame(master=root, width=525, height=225, border_width=0, corner_radius=15,
                                   fg_color="#222222", bg_color="#1e1e1e").place(x=713, y=242)
    global username_verify
    global password_verify
    global result
    global username_login_entry
    global password_login_entry
    username_verify = StringVar()
    password_verify = StringVar()
    username_login_entry = customtkinter.CTkEntry(None, width=400, height=45, fg_color="#222222", border_width=2, border_color="#0e67fb",
           text_font=('', 13, ""),text_color="white",placeholder_text=" Username ",placeholder_text_color="#707070",
           textvariable=username_verify).place(x=775, y=285)
    password_login_entry = customtkinter.CTkEntry(None, width=400, height=45, fg_color="#222222", border_width=2,border_color="#0e67fb",
           text_font=('', 13, ""),text_color="white",placeholder_text=" Password", placeholder_text_color="#707070",
           textvariable=password_verify, show='â—').place(x=775, y=375)
    login_B = customtkinter.CTkButton(master=root, width=350, height=50, border_width=0, border_color="",corner_radius=15,
           fg_color="#0453a2", bg_color="#1e1e1e", hover_color="#01407f", text="LogIn",text_font=('', 14, ""),text_color="white",
           compound="top",command=login_verify).place(x=800, y=500)
    home_B = Button(root, borderwidth=0, bg='#0453a2', width=3,height=1, text=" ðŸ”³ ", font=('', 25), fg='white',
           activebackground='#0453a2', activeforeground="white", command=Tap1).place(x=60, y=2)
    BG_gamenameHF = customtkinter.CTkFrame(master=root, width=575, height=80, border_width=0, corner_radius=15,
                 fg_color="#012951", bg_color="#151515").place(x=689,y=120)
    login_L=Label(root,text="Login", bg='#012951',font=('', 25), fg='white').place(x=720, y=140)

def login_verify():
    username1 = username_verify.get()
    password1 = password_verify.get()
    list_of_files = os.listdir()
    if username1 in list_of_files:
        file1 = open(username1, "r")
        verify = file1.read().splitlines()
        if password1 in verify:
            setting()
        else:
            login()


def stop():
    os.system("taskkill /im  Meta VR.exe")

def ref():
    return setting()

def stop():
    os.system("taskkill /im  MetaVR.exe")


#*******************************************  Game Setting   **************************************************************

def Beat_Saber():
    global gameImage
    global gameName
    global gamePath
    hour = StringVar()
    minute = StringVar()
    second = StringVar()
    hour.set("00")
    minute.set("07")
    second.set("00")
    gname = "Beat Saber.exe"
    def submit():
        os.startfile(r"d:\Software\Software\hyperbolic-magnetism-beat-saber\Beat Saber.exe")
        temp = int(hour.get()) * 3600 + int(minute.get()) * 60 + int(second.get())
        print(f'{temp /60} minute')
        while temp > -1:
            mins, secs = divmod(temp, 60)
            hours = 0
            if mins > 60:
                hours, mins = divmod(mins, 60)
            hour.set("{0:2d}".format(hours))
            minute.set("{0:2d}".format(mins))
            second.set("{0:2d}".format(secs))
            root.update()
            time.sleep(1)
            if (temp == 120):
                connect = sqlite3.connect("2000db.db")
                c = connect.cursor()
                c.execute("CREATE TABLE if not exists Beat_Saber (Game_ClickCount integer)")
                c.execute("INSERT INTO  Beat_Saber (Game_ClickCount) values (1)")
                c.execute('select * from Beat_Saber')
                result = c.fetchall()
                print(len(result))
                connect.commit()
                connect.close()
            if (temp == 0):
                messagebox.showinfo("Time Countdown", "Time's up ")
            temp -= 1
        else:
            os.system(f"taskkill /im  {gname}")
            # time.sleep(1)
            return Tap1()
    gameImage = PhotoImage(file="BEAT-SABER.png")
    gameName = str("Beat Saber")

    BG_F = customtkinter.CTkFrame(master=root, width=1920, height=1300, border_width=0, corner_radius=15,
                 fg_color="#101010", bg_color="#161616").place(x=0,y=75)
    BG_F1 = customtkinter.CTkFrame(master=root, width=570, height=590, border_width=0, corner_radius=15,
                 fg_color="#151515", bg_color="#101010").place(x=690,y=107)    #Ø§Ù„ÙƒØ¨ÙŠØ±
    BG_F1 = customtkinter.CTkFrame(master=root, width=550, height=470, border_width=0, corner_radius=15,
                 fg_color="#191919", bg_color="#151515").place(x=700,y=210)    #Ø§Ù„ØµÙˆØ±Ù‡
    BG_F1 = customtkinter.CTkFrame(master=root, width=390, height=245, border_width=0, corner_radius=15,
                 fg_color="#141414", bg_color="#191919").place(x=778,y=328)    # 1 Ø§Ù„ØµÙˆØ±Ù‡
    BG_gamenameHF = customtkinter.CTkFrame(master=root, width=550, height=80, border_width=0, corner_radius=15,
                 fg_color="#012951", bg_color="#151515").place(x=700,y=120)    #Ù‡ÙŠØ¯Ø± Ø§Ø³Ù… Ø§Ù„Ù„Ø¹Ø¨Ù‡
    BG_gamenameHFL = customtkinter.CTkLabel(master=root, width=1, text=gameName,text_color="white",text_font=('', 20,""),
                 fg_color="#012951", bg_color="#012951").place(x=730,y=142)    #Ø®Ù„ÙÙŠØ© Ø§Ø³Ù… Ø§Ù„Ù„Ø¹Ø¨Ù‡
    play_B = customtkinter.CTkButton(master=root,width=270,height=60,border_width=0,border_color="",corner_radius=15,
                 fg_color="#0453a2", bg_color="#1b1b1b", hover_color="#012951",text="Play Now",text_font=('',14,""),
                 text_color="white",command=submit).place(x=750,y=600)
    stop_B = customtkinter.CTkButton(master=root,width=150,height=60,border_width=0,border_color="",corner_radius=15,
                 fg_color="red", bg_color="#1b1b1b", hover_color="#012951",text="Stop Game",text_font=('',14,""),
                 text_color="white",command=stop).place(x=1050,y=600)
    BG_F2 = customtkinter.CTkFrame(master=root, width=500, height=100, border_width=0, corner_radius=15,
                 fg_color="#141414", bg_color="#191919").place(x=725,y=250)      # Ù‡ÙŠØ¯Ø± Ø§Ù„ØªØ§ÙŠÙ…Ø±
    BG_F2 = customtkinter.CTkFrame(master=root, width=475, height=75, border_width=0, corner_radius=15,
                 fg_color="#101010", bg_color="#141414").place(x=737,y=262)      # Ø®Ù„ÙÙŠØ© Ø§Ù„ØªØ§ÙŠÙ…Ø±
    entry = customtkinter.CTkEntry(None,width=60,height=60,textvariable=(hour),
                 fg_color="#101010",border_width=2,border_color="#0e67fb",text_font=('Verdana',25,"bold"),
                 text_color="white").place(x=870,y=270)
    entry = customtkinter.CTkEntry(None,width=60,height=60,textvariable=(minute),
                 fg_color="#101010",border_width=2,border_color="#0e67fb",text_font=('Verdana',25,"bold"),
                 text_color="white").place(x=940,y=270)
    entry = customtkinter.CTkEntry(None,width=60,height=60,textvariable=(second),
                 fg_color="#101010",border_width=2,border_color="#0e67fb",text_font=('Verdana',25,"bold"),
                 text_color="white").place(x=1010,y=270)
    gameImageL = Label(root, bg="#0453a2",borderwidth= 0, image=gameImage).place(x=790, y=352)

def Bullet_Train():
    global gameImage
    global gameName
    global gamePath
    hour = StringVar()
    minute = StringVar()
    second = StringVar()
    hour.set("00")
    minute.set("07")
    second.set("00")
    def submit():
        os.startfile(r"d:\Software\Software\epic-games-bullet-train-gdc\BulletTrain-GDC-1.8SDK\showup.exe")
        temp = int(hour.get()) * 3600 + int(minute.get()) * 60 + int(second.get())
        while temp > -1:
            mins, secs = divmod(temp, 60)
