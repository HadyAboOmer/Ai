def Into_The_Dead():
    global gameImage
    global gameName
    global gamePath
    hour = StringVar()
    minute = StringVar()
    second = StringVar()
    hour.set("00")
    minute.set("07")
    second.set("00")
    def submit():
        os.startfile(r"d:\Software\Software\pikpok-into-the-dead-rift\DR.exe")
        temp = int(hour.get()) * 3600 + int(minute.get()) * 60 + int(second.get())
        while temp > -1:
            mins, secs = divmod(temp, 60)
            hours = 0
            if mins > 60:
                hours, mins = divmod(mins, 60)
            hour.set("{0:2d}".format(hours))
            minute.set("{0:2d}".format(mins))
            second.set("{0:2d}".format(secs))
            root.update()
            time.sleep(1)
            if (temp == 120):
                connect = sqlite3.connect("2000db.db")
                c = connect.cursor()
                c.execute("CREATE TABLE if not exists Into_The_Dead (Game_ClickCount integer)")
                c.execute("INSERT INTO  Into_The_Dead (Game_ClickCount) values (1) ")
                c.execute('select * from Into_The_Dead')
                result = c.fetchall()
                print(len(result))
                connect.commit()
                connect.close()
            if (temp == 0):
                messagebox.showinfo("Time Countdown", "Time's up ")
            temp -= 1
        else:
            os.system("taskkill /im DR.exe")
            # time.sleep(1)
            return Tap1()
    gameImage = PhotoImage(file="INTO-THE-DEAD.png")
    gameName = str("Into The Dead")

    BG_F = customtkinter.CTkFrame(master=root, width=1920, height=1300, border_width=0, corner_radius=15,
                 fg_color="#101010", bg_color="#161616").place(x=0,y=75)
    BG_F1 = customtkinter.CTkFrame(master=root, width=570, height=590, border_width=0, corner_radius=15,
